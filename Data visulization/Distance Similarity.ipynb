   "source": [
    "# Ecludian Distance \n",
    "def euclidean_distance(x,y):\n",
    "    return sqrt(sum(pow(a-b,2) for a,b in zip(x,y)))\n",
    "\n",
    "a = [2,4,5,6,9]\n",
    "b = [3,4,6,7,-9]\n",
    "print(euclidean_distance(a,b))"
   ]
